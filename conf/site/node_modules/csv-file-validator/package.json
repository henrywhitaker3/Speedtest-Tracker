		"famulus": "^2.2.0",
		"lodash": "^4.17.20",
		"papaparse": "^5.3.0"
	},
	"devDependencies": {
		"ava": "^0.25.0",
		"codecov.io": "^0.1.6",
		"nyc": "^11.4.1"
	}
